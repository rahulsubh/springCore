<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.springcore.Student" name="student1">
		<property name="studentId" value="101" />
		<property name="studentName" value="Rahul" />
		<property name="studentAddress" value="Delhi" />
	</bean>
	
	<bean class = "com.springcore.Student" name = "student2" p:studentId = "102" p:studentName = "Shubh" p:studentAddress = "Bokaro"/>

</beans>